{
	"compilerOptions": {
		"sourceMap": false,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"allowJs": false,
		"strict": true,
		"noUnusedLocals": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"removeComments": false,
		"lib": ["esnext", "dom"]
	}
}
